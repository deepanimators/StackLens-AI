      "purpose": "maskable any"
    }
  ],
  
  "screenshots": [
    {
      "src": "/screenshots/desktop-dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Error Intelligence Dashboard"
    },
    {
      "src": "/screenshots/mobile-dashboard.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile Dashboard View"
    },
    {
      "src": "/screenshots/analytics-view.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Real-time Analytics"
    },
    {
      "src": "/screenshots/mobile-search.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile Search Interface"
    }
  ],
  
  "categories": [
    "productivity",
    "developer",
    "utilities",
    "business"
  ],
  
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View error intelligence dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/icons/shortcut-dashboard.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Search Errors",
      "short_name": "Search",
      "description": "Search and analyze errors",
      "url": "/search",
      "icons": [
        {
          "src": "/icons/shortcut-search.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View real-time analytics",
      "url": "/analytics",
      "icons": [
        {
          "src": "/icons/shortcut-analytics.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Upload Logs",
      "short_name": "Upload",
      "description": "Upload and analyze log files",
      "url": "/upload",
      "icons": [
        {
          "src": "/icons/shortcut-upload.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  
  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+stacklens",
      "url": "/handle?url=%s"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "logs",
          "accept": [".log", ".txt", ".json", "text/plain", "application/json"]
        }
      ]
    }
  },
  
  "launch_handler": {
    "client_mode": ["navigate-existing", "navigate-new"]
  },
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  
  "iarc_rating_id": "",
  
  "permissions": [
    "notifications",
    "background-sync",
    "periodic-background-sync"
  ],
  
  "features": [
    {
      "name": "Cross Platform",
      "description": "Works on desktop, mobile, and tablet devices"
    },
    {
      "name": "Offline Support",
      "description": "Continue working even when offline with local caching"
    },
    {
      "name": "Push Notifications",
      "description": "Get notified of critical errors and system alerts"
    },
    {
      "name": "Real-time Updates",
      "description": "Live error monitoring and analytics updates"
    },
    {
      "name": "AI-Powered Analysis",
      "description": "Intelligent error pattern detection and root cause analysis"
    }
  ]
}