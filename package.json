{
  "name": "stacklens-ai-platform",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "AI-powered error analysis and prediction platform with RAG capabilities",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "NODE_ENV=development node --import tsx --no-warnings server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "start": "NODE_ENV=production node --import tsx --no-warnings server/index.ts",
    "start:prod": "NODE_ENV=production HOST=0.0.0.0 node --import tsx --no-warnings server/index.ts",
    "preview": "vite preview",
    "check": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "db:migrate": "drizzle-kit migrate",
    "test": "echo 'Tests not implemented yet'",
    "clean": "rm -rf dist node_modules/.vite",
    "install:all": "npm install && pip install -r requirements.txt"
  },
