# StackLens AI - Simulation Mode Configuration
# Set this to enable simulation mode for all deep learning models

# Force simulation mode (overrides PyTorch detection)
STACKLENS_SIMULATION_MODE=true

# Simulation performance settings
SIMULATION_RESPONSE_DELAY_MS=100
SIMULATION_ACCURACY_RANGE=0.85-0.95
SIMULATION_CONFIDENCE_RANGE=0.80-0.95

# Logging for simulation mode
LOG_SIMULATION_PREDICTIONS=true
SIMULATION_DEBUG_MODE=true
